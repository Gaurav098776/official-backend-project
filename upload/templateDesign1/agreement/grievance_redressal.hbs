<html>
  <header>
    <meta http-equiv='Content-Type' content='text/html; charset=UTF-8' />
    <link rel='preconnect' href='https://fonts.googleapis.com' />
    <link rel='preconnect' href='https://fonts.gstatic.com' crossorigin />
    <link
      href='https://fonts.googleapis.com/css2?family=Lexend&family=Lexend+Deca:wght@200&display=swap'
      rel='stylesheet'
    />
    <style>
      .body { font-family: "Lexend"; }
    </style>
  </header>
  <body style='padding: 0; margin: 0'>
    <table
      style='
        width: 100%;
        margin: auto;
        border-collapse: collapse;
        font-family: Lexend;
      '
    >
      <tbody>
        <tr>
          <td>
            <table
              style='
                max-width: 725px;
                width: 100%;
                margin: auto;
                border-collapse: collapse;
                font-family: Lexend;
              '
            >
              <tbody>
                <tr>
                  <td style='height: 80px'>
                    <div style='margin-top: 18px; margin-bottom: 8px'>
                      <img width='165px' height='40px' src={{nbfcLogo}} />
                    </div>
                    {{#cIf showLendingPartner}}
                      <div style='font-size: 12px; margin-bottom: 18px'>
                        Lending Partner :
                        {{appName}}
                      </div>
                    {{/cIf}}
                  </td>
                  <td style='padding: 0 0px 0 0; width: 400px'>
                    <div
                      style='
                        font-size: 12px;
                        text-align: end;
                        margin: 5px 0 0 0;
                      '
                    >
                      {{nbfcAddress}}
                    </div>
                  </td>
                </tr>
              </tbody>
            </table>
          </td>
        </tr>
        <tr>
          <td
            style='background-color: #e02d42; height: 2px; margin-top: -2px'
          ></td>
        </tr>

        {{#if referenceData.length}}
          <tr>
            <td
              style='background-color: #3bcbcc; height: 10px; margin-top: -2px'
            ></td>
          </tr>
          <tr>
            <td style='padding: 10px 30px 30px 30px'>
              <table
                style='
                background-color: #fff;
                margin: auto;
                width: 100%;
                font-family: Lexend;
                border-collapse: collapse;
              '
              >
                <tbody>
                  <tr>
                    <td
                      style='
                      padding: 25px 5px 10px 5px;
                      color: #000;
                      font-size: 13px;
                    '
                      colspan='4'
                    >
                      Borrower agrees and provides permission to the Lender or
                      its agents to get in touch with given reference if
                      borrower is not reachable.
                    </td>
                  </tr>
                  {{#if usedReferences}}
                    <tr>
                      <td
                        style='
                      padding: 0px 5px 10px 5px;
                      color: #000;
                      font-size: 13px;
                    '
                        colspan='4'
                      >
                        {{usedReferences}}
                      </td>
                    </tr>
                  {{/if}}
                  <tr>
                    <td
                      style='
                      border: 1px solid #000;
                      padding: 8px 10px;
                      font-size: 12px;
                    '
                    >
                      Name of the Reference
                    </td>
                    <td
                      style='
                      border: 1px solid #000;
                      padding: 8px 10px;
                      font-size: 12px;
                    '
                    >
                      Phone number
                    </td>
                    <td
                      style='
                      border: 1px solid #000;
                      padding: 8px 10px;
                      font-size: 12px;
                    '
                    >
                      Permission Date /Time
                    </td>
                    <td
                      style='
                      border: 1px solid #000;
                      padding: 8px 10px;
                      font-size: 12px;
                    '
                    >
                      Given By
                    </td>
                  </tr>
                  {{#each referenceData}}
                    <tr>
                      <td
                        style='
                      border: 1px solid #000;
                      padding: 8px 10px;
                      font-size: 12px;
                    '
                      >
                        {{this.refName}}
                      </td>
                      <td
                        style='
                      border: 1px solid #000;
                      padding: 8px 10px;
                      font-size: 12px;
                    '
                      >
                        {{this.refPhone}}
                      </td>
                      <td
                        style='
                      border: 1px solid #000;
                      padding: 8px 10px;
                      font-size: 12px;
                    '
                      >
                        {{this.refPermissionDate}}
                      </td>
                      <td
                        style='
                      border: 1px solid #000;
                      padding: 8px 10px;
                      font-size: 12px;
                    '
                      >
                        {{this.refGivenBy}}
                      </td>
                    </tr>
                  {{/each}}
                </tbody>
              </table>
            </td>
          </tr>
        {{/if}}

        <tr>
          <td>
            <div
              style='
                padding: 10px 30px 10px 30px;
                text-align: right;
                font-size: 10px;
              '
            >
              <b>In continuation to page 1</b>
            </div>
          </td>
        </tr>

        <tr style='font-size: 12px; margin: 40px 30px 15px 30px; display: flex'>
          <td>
            <div>
              <strong>ROI Processing Fees & Other Charges â€“ Approach</strong>
            </div>
            <p>
              To ensure transparency, in conformity with the stipulations of the
              Reserve Bank of India's (hereafter referred to as 'RBI')
              directives,
              {{nbfcName}}
              has adopted the following approach:
            </p>
            <div>
              <strong>INTEREST RATE MODEL AND APPROACH FOR GRADATION OF RISK
                ASSESSMENT</strong>
            </div>
            <p>
              The interest rate to be charged to the borrower for the loans will
              be decided keeping in view the RBI's guidelines. Interest rate
              will be arrived based on the following broad parameters:
            </p>
            <div>
              <ol type='a'>
                <li>Risk profile of the borrower;</li>
                <br />
                <li>Interest rate trend prevailing in the money market;</li>
                <br />
                <li>Cost of borrowings;</li>
                <br />
                <li>Interest rate charged by competitors;</li>
                <br />
                <li>Tenure of the borrowing;</li>
                <br />
                <li>Historical track record of the borrower with the {{nbfcName}};</li>
                <br />
                <li>Operations/Administrative cost and profit margin.</li>
              </ol>
            </div>
            <p>
              The rate of interest for the same product and tenor availed during
              same period by different borrower need not be uniform. It could
              vary for the same or different borrower.
            </p>
            <p>
              For more details, please refer to our Interest Rate & Fee Policy
              documented on our website
              <a href={{nbfcIrmodel}} target='_blank'>{{nbfcIrmodel}}</a>
            </p>
          </td>
        </tr>
        <!-- Repayment table -->
        <tr style='font-size: 12px; margin: 15px 30px 15px 30px; display: flex'>
          <td>
            <div>
              <strong>Loan repayment schedule:</strong>
            </div>
          </td>
        </tr>
        <tr>
          <td style='padding: 10px 30px 30px 30px'>
            <table
              style='
                background-color: #fff;
                margin: auto;
                width: 100%;
                font-family: Lexend;
                border-collapse: collapse;
              '
            >
              <thead>
                <tr style='background-color: #d9d9d9'>
                  <th
                    style='
                      border: 1px solid #000;
                      padding: 7px 10px;
                      font-size: 12px;
                      font-weight: bold;
                    '
                  >
                    Serial No.
                  </th>
                  <th
                    style='
                      border: 1px solid #000;
                      padding: 7px 10px;
                      font-size: 12px;
                      font-weight: bold;
                      min-width: 75px;
                    '
                  >
                    Due date
                  </th>
                  <th
                    style='
                      border: 1px solid #000;
                      padding: 7px 10px;
                      font-size: 12px;
                      font-weight: bold;
                    '
                  >
                    Principal amount
                  </th>
                  <th
                    style='
                      border: 1px solid #000;
                      padding: 7px 10px;
                      font-size: 12px;
                      font-weight: bold;
                    '
                  >
                    Interest amount
                  </th>
                  <th
                    style='
                      border: 1px solid #000;
                      padding: 7px 10px;
                      font-size: 12px;
                      font-weight: bold;
                    '
                  >
                    No of days interest calculated
                  </th>
                  <th
                    style='
                      border: 1px solid #000;
                      padding: 7px 10px;
                      font-size: 12px;
                      font-weight: bold;
                    '
                  >
                    Rate of interest (per day)
                  </th>
                  <th
                    style='
                      border: 1px solid #000;
                      padding: 7px 10px;
                      font-size: 12px;
                      font-weight: bold;
                    '
                  >
                    Total Repayment amount
                  </th>
                </tr>
              </thead>
              <tbody>
                {{#each emiData}}
                  <tr>
                    <td
                      style='
                      border: 1px solid #000;
                      border-collapse: collapse;
                      height: 25px;
                      text-align: center;
                      font-size: 12px;
                    '
                    >
                      {{this.id}}
                    </td>
                    <td
                      style='
                      border: 1px solid #000;
                      border-collapse: collapse;
                      text-align: center;
                      font-size: 12px;
                    '
                    >
                      {{this.dueDate}}
                    </td>
                    <td
                      style='
                      border: 1px solid #000;
                      border-collapse: collapse;
                      text-align: center;
                      font-size: 12px;
                    '
                    >
                      {{this.principal}}
                    </td>

                    <td
                      style='
                      border: 1px solid #000;
                      border-collapse: collapse;
                      text-align: center;
                      font-size: 12px;
                    '
                    >
                      {{this.interest}}
                    </td>

                    <td
                      style='
                      border: 1px solid #000;
                      border-collapse: collapse;
                      text-align: center;
                      font-size: 12px;
                    '
                    >
                      {{this.days}}
                    </td>
                    <td
                      style='
                      border: 1px solid #000;
                      border-collapse: collapse;
                      text-align: center;
                      font-size: 12px;
                    '
                    >
                      {{this.interestRate}}
                    </td>
                    <td
                      style='
                      border: 1px solid #000;
                      border-collapse: collapse;
                      text-align: center;
                      font-size: 12px;
                    '
                    >
                      {{this.dueAmount}}
                    </td>
                  </tr>
                {{/each}}
                <tr>
                  <td
                    colspan='2'
                    style='
                      border: 1px solid #000;
                      border-collapse: collapse;
                      height: 25px;
                      text-align: center;
                      font-size: 12px;
                    '
                  >
                    Total
                  </td>

                  <td
                    style='
                      border: 1px solid #000;
                      border-collapse: collapse;
                      text-align: center;
                      font-size: 12px;
                    '
                  >
                    {{emiTotalPrincipal}}
                  </td>

                  <td
                    style='
                      border: 1px solid #000;
                      border-collapse: collapse;
                      text-align: center;
                      font-size: 12px;
                    '
                  >
                    {{emiTotalInterest}}
                  </td>

                  <td
                    style='
                      border: 1px solid #000;
                      border-collapse: collapse;
                      text-align: center;
                      font-size: 12px;
                    '
                  >
                    {{emiTotalDays}}
                  </td>

                  <td
                    style='
                      border: 1px solid #000;
                      border-collapse: collapse;
                      text-align: center;
                      font-size: 12px;
                    '
                  >
                    {{emiInterestRate}}
                  </td>
                  <td
                    style='
                      border: 1px solid #000;
                      border-collapse: collapse;
                      text-align: center;
                      font-size: 12px;
                    '
                  >
                    {{emiTotalAmount}}
                  </td>
                </tr>
              </tbody>
            </table>
          </td>
        </tr>
        <!-- Repayment table End -->
      </tbody>
    </table>
    <div
      style='font-size: 10px; bottom: 5px; position: fixed; right: 0; padding: 10px 30px 5px 0px; text-align: right;'
    >
      <b>Page 2</b>
    </div>
  </body>
</html>