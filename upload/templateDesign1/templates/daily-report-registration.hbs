<html>
  <head>
    <style type='text/css'>
      html, body { margin: 0; padding: 0; font-family: Work Sans, sans-serif; }
      table { width: 100%; border-collapse: collapse; } th, td { padding: 10px
      10px; border-collapse: collapse; font-size: 12px; } .border-table th,
      .border-table td { border: 1px solid #e6e6e6; }
    </style>
  </head>

  <body>
    <div style='width: 768px; margin: auto'>
      <table>
        <tr>
          <td
            style='
              background-color: #ffffff;
              text-align: center;
              padding: 10px 0 15px;
            '
          >
            <img
              width='125px'
              src='{{nbfcLogo}}'
              class='CToWUd'
              data-bit='iit'
            />
          </td>
        </tr>
        <tr>
          <td style='text-align: center; padding: 15px 0'>
            <h3 style='margin: 0; font-size: 16px'>
              Registration summary :
              {{reportDate}}
            </h3>
          </td>
        </tr>

        <!-- table  -->
        <tr>
          <td>
            <table class='border-table'>
              <tr>
                <th
                  style='
                    background-color: #252f75;
                    color: #fff;
                    text-align: center;
                  '
                >
                  Platform
                </th>
                <th
                  style='
                    background-color: #252f75;
                    color: #fff;
                    text-align: center;
                  '
                ></th>
                <th
                  style='
                    background-color: #252f75;
                    color: #fff;
                    text-align: center;
                  '
                >
                  Today
                </th>
                <th
                  style='
                    background-color: #252f75;
                    color: #fff;
                    text-align: center;
                  '
                >
                  Current month({{comparisonData.compCurrMonthDate}})
                </th>
                <th
                  style='
                    background-color: #252f75;
                    color: #fff;
                    text-align: center;
                  '
                >
                  Last month({{comparisonData.compLastMonthDate}})
                </th>
              </tr>
              {{#each userData}}
                <tr>
                  <th rowspan='2'>{{this.appType}}</th>
                  <th>Android</th>
                  <th>{{this.todaysUser.android}}</th>
                  <th>{{this.currentMonthUser.android}}</th>
                  <th>{{this.lastMonthUser.android}}</th>
                </tr>

                <tr>
                  <th>iOS</th>
                  <th>{{this.todaysUser.ios}}</th>
                  <th>{{this.currentMonthUser.ios}}</th>
                  <th>{{this.lastMonthUser.ios}}</th>
                </tr>

                <tr style='background-color: #f8f8f8'>
                  <th>Total</th>
                  <th></th>
                  <th>{{this.todaysUser.todayTotal}}</th>
                  <th>{{this.currentMonthUser.currMonthTotal}}</th>
                  <th>{{this.lastMonthUser.lastMonthTotal}}</th>
                </tr>
              {{/each}}
            </table>
          </td>
        </tr>
        <tr>
          <td>
            <hr />
          </td>
        </tr>

        <!-- Footer -->
        <tr>
          <td>
            <tr style='text-align: center'>
              <td
                style='
                  background-color: #252f75;
                  color: #ffffff;
                  padding: 10px 0 0 0;
                '
              >
                Partnered NBFC :
                <span style='font-weight: bold'> {{nbfcName}} </span>
                / NBFC registration number :
                <span style='font-weight: bold'>
                  {{nbfcRegisterationNumber}}
                </span>
              </td>
            </tr>
            <tr style='text-align: center'>
              <td
                style='
                  background-color: #252f75;
                  color: #ffffff;
                  padding: 5px 0 10px 0;
                '
              >
                {{nbfcAddress}}
              </td>
            </tr>
          </td>
        </tr>
      </table>
    </div>
  </body>
</html>